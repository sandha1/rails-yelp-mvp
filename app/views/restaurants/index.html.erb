<h1>Restaurants#index</h1>

<ul>
  <% @restaurants.each do |restaurant|  %>
    <li>
      <h2><%= link_to restaurant.name, restaurant_path(restaurant) %></h2>
      <p><%= restaurant.address %></p>
      <p><%= restaurant.category %></p>
    </li>
  <% end %>
</ul>

<%= link_to "New restaurant", new_restaurant_path %>
